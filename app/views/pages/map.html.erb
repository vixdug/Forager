<div class='column' id="map-scroll">
  <div class="ui three item menu">
    <div class="item">
      <div class="ui buttons">
        <button class="ui olive ui button fruit" >Fruit</button>
        <div class="or"></div>
        <button class="ui olive ui button veg">Vegetable</button>
      </div>
    </div>
