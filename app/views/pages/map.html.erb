            </div>
            <div class="item" data-value="All">
              <div class="ui empty circular label"></div>
              All
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

        <div class="ui centered link cards">

          <% @marker.each do | marker | %>

          <% if marker.name.upcase == "LETTUCE" %>
          <div class="teal card" name= <%= marker.id %> id= <%= marker.category %> >
            <% else %>
              <div class="card" name= <%= marker.id %> id= <%= marker.category %>>
                <% end %>

            <div class="image">
              <% if marker.name.upcase == "LETTUCE" %>
              <img src="https://cdn.shopify.com/s/files/1/0790/5353/files/lettuce.png?7058430645863851559">
              <% end %>

              <% if marker.name.upcase == "TOMATO" %>
                  <img src="  https://cdn.shopify.com/s/files/1/0790/5353/files/lemon_978d4dcc-8bc7-488c-8914-4e351748ddb5.png?15897903740619616183">
                  <% end %>

                <% if marker.name == "Strawberries" || marker.name == "berry" || marker.name == "strawberry" %>
                <img src="https://cdn.shopify.com/s/files/1/0790/5353/files/berry.png?10987933923783252097">
                <% end %>

                <% if marker.name.upcase == "APPLES" %>
                  <img src="https://cdn.shopify.com/s/files/1/0790/5353/files/Untitled-2.png?14227909331024637198">
                  <% end %>

                  <% if marker.name.upcase == "CARROTS" %>
                    <img src="https://cdn.shopify.com/s/files/1/0790/5353/files/carrot.png?14706481068821637579">
                    <% end %>

                    <% if marker.name.upcase == "CHERRIES" || marker.name == "CHERRY" %>
                    <img src="https://cdn.shopify.com/s/files/1/0790/5353/files/cherry.png?15324602097155618717">
                <% end %>

                <% if marker.name.upcase == "GRAPE" %>
                <img src="https://cdn.shopify.com/s/files/1/0790/5353/files/grape.png?15928860166532810551">
            <% end %>
              </div>
            <div class="content">
              <div class="header"><%= marker.name %> </div>
              <div class="meta">
                <a><%= marker.latitude %> <%= marker.longitude  %> </a>
              </div>
              <div class="description">
              description description foraging description
              </div>
            </div>
            <div class="extra content">
              <span class="right floated">
                <%= marker.category %>
              </span>
              <span>
                <%= link_to like_marker_path(marker), method: :put do %>
                <i class="heart icon"></i>
              <%= marker.get_upvotes.size %> Friends
                <%  end %>
              </span>
            </div>
          </div>
          <% end %>


